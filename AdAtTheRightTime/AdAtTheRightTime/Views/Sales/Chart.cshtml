@{
    SortedDictionary<DateTime, Double> DataPointsSansOneYear = new SortedDictionary<DateTime, Double>();

    foreach (var item in Model)
    {
        DateTime date = Convert.ToDateTime(item.Name);
        Double totalNumber = Convert.ToDouble (item.Total__c);
        if (date > DateTime.Today.AddYears(-1).AddDays(-30)){
            DataPointsSansOneYear.Add(date, totalNumber);
        }

    }
    var myChart = new Chart(width: 600, height: 400).AddTitle("Comparison Chart");
    myChart.AddSeries(
        chartType: "Line",
        name: "Sales",

        xValue: DataPointsSansOneYear.Keys,
        yValues: DataPointsSansOneYear.Values);
    myChart.SetXAxis("Date",DateTime.Today.AddYears(-1).AddDays(-30).ToOADate());
    myChart.SetYAxis("Sales in $");
    myChart.Write();
}